{
    // login
    "useHashLoginPassword": true, // changing value when your app has been go live would be trigger fail login
    "hashPasswordAlgorithm": "md5",
    "customLoginUrl": null, //"login.php",
    "customLogoutUrl": null, //"logout.php",

    // date and time format
    "dateFormat": "d M Y, H:i:s",
    "timeFormat": "H:i",
    "submitDateFormat": "Y-m-d H:i:s",
    "submitTimeFormat": "H:i:s",
    "comboLimit": 100000,
    "gridLimit": 50,

    // general
    "notifInterval": 180000, // 1 minute in miliseconds
    "maxPageCount": 10,
    "encryptedRuleAccess": true,

    // use it on development mode only
    "traceEvents": false,

    // assets
    "handout": "handout",
    "mobile": "mobile",

    //Config multi profile
    "multiProfileIsActive" : true,

    "runtimeWeb" : true,

    "protocol": "https",
    "emitterKeygenChannelHost": "sse.lab.sipas.id",
    "emitterKeygenChannelPort": "",
    "emitterServiceApi": "/request-topic",
    "groupToSubsribe": ["staf", "asistensi", "unit"],
    "devMode": ["publish", "dev", "sandbox"]
}