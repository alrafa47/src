                            "    case record.self.statusBerkas().BERKAS_REQUEST:",
                            "        return new Ext.XTemplate(['<div class=\"cell-text\">',",
                            "                                  '<div class=\"subtext warning\">',",
                            "                                  '<span class=\"badge badge-solid margin-right-4\">',",
