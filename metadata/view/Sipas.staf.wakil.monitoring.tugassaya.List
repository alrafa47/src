            "codeClass": null,
            "userConfig": {
                "dataIndex": "surat_tipe",
                "sortable": false,
                "text": "Tipe Surat",
                "width": 140
            },
            "name": "MyColumn18",
            "designerId": "9c9c73a3-3207-4e56-9f1a-7a94f3a3247e",
            "cn": [
                {
                    "type": "fixedfunction",
                    "reference": {
                        "name": "items",
                        "type": "array"
                    },
                    "codeClass": null,
                    "userConfig": {
                        "designer|params": [
                            "value",
                            "metaData",
                            "record",
                            "rowIndex",
                            "colIndex",
                            "store",
                            "view"
                        ],
                        "fn": "renderer",
                        "implHandler": [
                            "var surat_model = record.get('surat_model'),",
                            "    mode = record.get('disposisi_mode'),",
                            "    is_baca = record.get('disposisi_masuk_isbaca'),",
                            "    is_cabut = record.get('disposisi_masuk_iscabut'),",
                            "    jenis = record.get('jenis_nama') ? record.get('jenis_nama') : '-',",
                            "    tipe = '',",
                            "    baca = '';",
                            "",
                            "if(is_baca === 1){",
                            "    if(is_cabut === 1){",
                            "        baca = ' red-700-i';",
                            "    }else{",
                            "        baca = ' grey-700-i';",
                            "    }",
                            "}else if(is_baca === 0){",
                            "    if(is_cabut === 1){",
                            "        baca = ' bold red-700-i';",
                            "    }else{",
                            "        baca = ' bold grey-700-i';",
                            "    }",
                            "}",
                            "",
                            "if (mode === 'Disposisi'){",
                            "    tipe = 'Disposisi';",
                            "} else if (mode === 'Nota Dinas'){",
                            "    tipe = 'Nota Dinas';",
                            "} else if (mode === 'Draf'){",
                            "    if (surat_model === 2){",
                            "        tipe = 'Koreksi Eksternal';",
                            "    } else if (surat_model === 4){",
                            "        tipe = 'Koreksi Internal';",
                            "    } else if (surat_model === 6){",
                            "        tipe = 'Koreksi Keputusan';",
                            "    }",
                            "} else if (mode == 'Petikan') {",
                            "    tipe = 'Koreksi Petikan';",
                            "} else {",
                            "    if (surat_model === 1){",
                            "        tipe = 'Masuk Eksternal';",
                            "    } else if (surat_model === 3){",
                            "        tipe = 'Masuk Internal';",
                            "    } else if (surat_model === 2){",
                            "        tipe = 'Keluar Eksternal';",
                            "    }",
                            "}",
                            "return new Ext.XTemplate(['<div class=\"cell-text\"><div class=\"subtext{is_baca}\">{sub}</div><div class=\"supporttext{is_baca}\">{support}</div></div>']).apply({",
                            "    sub: jenis,",
                            "    support: tipe,",
                            "    is_baca: baca",
                            "});"
                        ]
                    },
                    "name": "renderer",
                    "designerId": "9a219f1b-827f-4038-9e9a-360f5d24d067"
                }
            ]
        },
        {
