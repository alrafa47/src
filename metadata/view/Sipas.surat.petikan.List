                            "    if(value){",
                            "        ttd= \"<img src='server.php/sipas/staf/get_ttd?id=\"+value+\"' style='width:100px; height:50px;'>\";",
                            "        if(idStaf != value){",
                            "            via_asistensi = '<div class=\"supporttext supporttext-dark\">Via asistensi oleh <span class=\"bold\">'+pelakuNama+'</span></div>';",
                            "        }",
                            "    }",
                            "}",
                            "return ttd+via_asistensi;"
