                            "        return '<i class=\"\"></i>'+ aktifOrKadaluarsa +' <span style=\"color:#999999;\">'+ separator +' '+tanggal+'</span>';",
                            "    }",
                            "}else{",
                            "    return '<i class=\"\"></i><span>Tanpa masa aktif</span>';",
                            "}"
                        ]
