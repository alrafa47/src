            "cn": [
                {
                    "type": "fixedfunction",
                    "reference": {
                        "name": "items",
                        "type": "array"
                    },
                    "codeClass": null,
                    "userConfig": {
                        "designer|params": [
                            "value",
                            "metaData",
                            "record",
                            "rowIndex",
                            "colIndex",
                            "store",
                            "view"
                        ],
                        "fn": "renderer",
                        "implHandler": [
                            "var val = \"<div class='subtext'>\"+value+\"</div>\",",
                            "    jam = \"<div class='supporttext supporttext-dark'>\"+ Ext.util.Format.date(record.get('surat_properti_buat_tgl'), 'd M Y H:i') +\"</div>\",",
                            "    _return = '';",
                            "",
                            "if(record.get('surat_sorter') === 0){",
                            "    if(value){",
                            "        _return = '<span class=\"bold\">'+val+jam+'</span>';",
                            "    }else{",
                            "        _return = '<span class=\"alternative bold\">-</span>';",
                            "    }",
                            "}else{",
                            "    if(value){",
