            "            ",
            "        if (use_retensi){",
            "            retensi_tgl.setHours(0,0,0,0);",
            "            tanggal.setHours(0,0,0,0);",
            "            var isretensi = (retensi_tgl < tanggal)? 1 : 0;",
            "",
            "            if(isretensi){",
            "                return \"x-grid-row-alternative\";",
